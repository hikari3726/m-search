<!DOCTYPE html>
<html>
  <head>
    <title>MSearch</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar">
      <div class="container" >
        <div class="navbar-left">
          <%= link_to "M-Search", "#", class: 'navbar-white' %>
        </div>
        <div class="navbar-right">
          <ul>
            <li>
              <%= link_to "サインイン", "#", class: 'navbar-white' %>
            </li>
            <li>
              <%= link_to "ログイン", "#", class: 'navbar-white' %>
            </li>
          </ul>
        </div>
      </div>
    </nav>
    
    <%= yield %>
  </body>
</html>
